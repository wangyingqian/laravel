{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "type": "project",
    "repositories": [
        {
            "type" : "git",
            "url" : "git@github.com:wangyingqian/Lawechat.git"
        },
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.6",
        "laravel/tinker": "~1.0",
        "predis/predis": "1.0",
        "guzzlehttp/guzzle": "6.3.*"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "whoops/soap": "^1.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "lawechat/framework":"*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
